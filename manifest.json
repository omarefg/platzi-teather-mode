{
  "name": "Platzi Teather Mode",
  "desription": "Te permite active el modo teatro en los videos de los cursos de Platzi",
  "manifest_version": 3,
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "icons": {
    "16": "/assets/16.png",
    "32": "/assets/32.png",
    "48": "/assets/48.png",
    "128": "/assets/128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/assets/16.png",
      "32": "/assets/32.png",
      "48": "/assets/48.png",
      "128": "/assets/128.png"
    }
  }
}
